@model IEnumerable<TESTEBACKEND.Models.Clinica>;
@{
    ViewData["Title"] = "mostrarEspecialidade";
}

@foreach (var item in Model) 
{
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">@item.NomeClinica<h5>
            <h6 class="card-subtitle mb-2 text-body-secondary">@item.Especialidade</h6>
            <p class="card-text">@item.Descricao</p>
            <a href="#" class="card-link">Card link</a>
            <a href="#" class="card-link">Another link</a>
        </div>
    </div>
}
